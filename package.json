{
  "name": "climate-by-location",
  "version": "2.2.0",
  "description": "This library defines functions that can create and manage interactive climate widget graphs.",
  "main": "climate-by-location.js",
  "scripts": {
    "build": "rollup -c && cat build/climate-by-location.js > climate-by-location.js",
    "watch": "nodemon --watch src --exec 'npm run build'",
    "start": "nodemon --watch src --exec 'npm run build && node serve.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nemac/climate-by-location.git"
  },
  "bugs": {
    "url": "https://github.com/nemac/climate-by-location/issues"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "core-js-bundle": "^3.6.5",
    "unfetch": "^4.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@rollup/plugin-babel": "^5.1.0",
    "babel-plugin-transform-es2015-literals": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "lodash-es": "^4.17.15",
    "node-static": "~0.7.11",
    "nodemon": "^2.0.4",
    "rollup": "^2.23.0"
  }
}
